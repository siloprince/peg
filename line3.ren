自然数 [0] := '+1
パイの素A [1][3] := 6 ' +  (2自然数-1)(2自然数-1) ''
パイの素B [0][1] := 6 ' +  (2自然数-1)(2自然数-1) ''
パイ := パイの素A/パイの素B	
辺数 := 5
外角 := 2パイ#/辺数
コサインの素 [1] := -' 外角 外角 / (2自然数(2自然数-1)) 
コサイン [1] := ' + コサインの素 
サインの素 [外角#] := -2 ' 外角 外角 / (2自然数(2自然数+1)) 
サイン [外角#] := ' + サインの素 
コサインN倍角 [コサイン#][0] := 2コサイン# ' - '' 
サインN倍角 [-サイン#][0] := 2コサイン# ' - '' 
辺長 := 20
回転数 := 1
辺長 := 20
点X座標 [0] := '-辺長 コサインN倍角 | ( 自然数 <= 辺数 回転数 )